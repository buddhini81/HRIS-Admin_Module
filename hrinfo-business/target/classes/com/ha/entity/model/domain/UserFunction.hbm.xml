<?xml version="1.0"?>
            <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ha.entity.model.domain.UserFunction" table="userfunction">
        <id name="userFunctionDid"
                    column="userfunctiondid"
                    type="java.lang.Long">
        </id>

        <property name="userRoleDid"
                    column="userroledid"
                    type="java.lang.Long"
                    />
        <property name="functionDid"
                    column="functiondid"
                    type="java.lang.Long"
                    />

    </class>

</hibernate-mapping>

