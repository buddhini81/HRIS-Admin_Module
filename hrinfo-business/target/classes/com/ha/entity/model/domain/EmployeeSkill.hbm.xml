<?xml version="1.0"?>
            <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ha.entity.model.domain.EmployeeSkill" table="employeeskill">
        <id name="skillDid"
                    column="skillDid"
                    type="java.lang.Long">
                    <generator class="increment"/>
        </id>

        <property name="skillTypeDid"
                    column="skillTypeDid"
                    type="java.lang.Long"
                    />
        <property name="employeeDid"
                    column="employeeDid"
                    type="java.lang.Long"
                    />
        <property name="description"
                    column="description"
                    type="java.lang.String"
                    />
        

    </class>

</hibernate-mapping>

