<?xml version="1.0"?>
            <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ha.entity.model.domain.CompositeAddedAttribute" table="compositeaddedattribute">
        <id name="compositeAddedAttributeDid"
                    column="compositeaddedattributedid"
                    type="java.lang.Long">
                   <generator class="increment"/>
        </id>
        
        <property name="attributeGroupDid"
                    column="attributegroupdid"
                    type="java.lang.Long"/>
                    
        <property name="attributeSubGroupDid"
                    column="attributesubgroupdid"
                    type="java.lang.Long"/>

        <property name="did"
                    column="DID"
                    type="java.lang.Long"
                    />
        <property name="tObject"
                    column="tObject"
                    type="java.lang.String"/>

    </class>

</hibernate-mapping>