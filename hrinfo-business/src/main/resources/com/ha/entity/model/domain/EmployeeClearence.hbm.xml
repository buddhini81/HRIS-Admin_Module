                    column="workHandedOver"
                    type="java.lang.Boolean"
                    />
        <property name="companyPropertyHandedOver"
                    column="companyPropertyHandedOver"
                    type="java.lang.Boolean"
                    />
		<property name="financialClearenceDone"
                    column="financialClearenceDone"
                    type="java.lang.Boolean"
                    />
        <property name="serviceLetterIssued"
                    column="serviceLetterIssued"
                    type="java.lang.Boolean"
                    />
        <property name="bCardHandedOver"
                    column="bcardHandedOver"
                    type="java.lang.Boolean"
                    />
         <property name="grativityPaid"
                    column="grativityPaid"
                    type="java.lang.Boolean"
                    />
    </class>

</hibernate-mapping>

